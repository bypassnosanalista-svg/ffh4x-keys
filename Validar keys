<?php
function gerarKey() {
    $prefixo = "FFH4X-ANDROID-";
    $chars = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
    $random = "";

    for ($i = 0; $i < 6; $i++) {
        $random .= $chars[rand(0, strlen($chars) - 1)];
    }

    return $prefixo . $random;
}

$key = gerarKey();

// Salva a key em um arquivo (lista branca)
file_put_contents("keys.txt", $key . PHP_EOL, FILE_APPEND);

echo "KEY GERADA: " . $key;
?>
